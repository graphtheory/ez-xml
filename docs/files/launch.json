    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Example test",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "env": {
                "NODE_ENV": "development"
            },
            "args": [
                "-u", "tdd",
                "--timeout", "999999",
                "--colors",
                "./build/test/example_test.js"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "stopOnEntry": false,
            "sourceMaps": true,
            "cwd": "${workspaceRoot}", 
            "outFiles": [
                "${workspaceRoot}/build/**/*.js"
            ]
        },
